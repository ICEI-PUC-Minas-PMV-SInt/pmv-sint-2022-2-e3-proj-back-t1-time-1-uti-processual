@model utip_backend.Models.ProcessModel
@{
    ViewData["Title"] = "Dashboard";
}

<div>
    <h1>Lei 11.343/06</h1>

    <div>
        <table class="table table-dark table-striped">
            <thead>
                <tr>
                    <td scope="col">Num Processo</td>
                    <td scope="col">Status</td>
                    <td scope="col">color</td>
                    <td scope="col">Ações</td>
                </tr>
            </thead>
            <tbody>
                @foreach(var item in ) //Definir propriedade GetEnumerate//
                {
                    <tr>
                        <td>@Html.DisplayFor(modelItem => item.NumLegalProcess)</td>
                        <td>@Html.DisplayFor(modelItem => item.TipoPenal)</td>
                        <td>@Html.DisplayFor(modelItem => item.IdStatus)</td>
                        <td>
                            <div>
                                @Html.ActionLink("Edit", "Edit", new { id = item.ID })
                                @Html.ActionLink("Delete", "Delete", new { id = item.ID })
                            </div>
                        </td>
                    </tr>
                }
                
            </tbody>
        </table>
    </div>
</div>
